% first order logic operator symbols for syntax
\def\Not{\neg}
\def\And{\wedge}
\providecommand{\bigAnd}{\bigwedge}
\def\Or{\vee}
\providecommand{\bigOr}{\bigvee}

\providecommand{\Implies}{\Rightarrow}
\providecommand{\Isimplied}{\Leftarrow}
\providecommand{\Equiv}{\Leftrightarrow}
\providecommand{\All}[2]{\forall #1\left(#2\right)}
\providecommand{\Exi}[2]{\exists #1\left(#2\right)}

\providecommand{\True}{\text{\itshape true}}
\providecommand{\False}{\text{\itshape false}}
